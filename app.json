{
  "expo": {
    "name": "mediplus",
    "description": "Revolutionising your trip to the doctor, one screen at a time.",
    "privacy": "public",
    "slug": "mediplus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/BlueLogoTrim.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "githubUrl": "https://github.com/timTam97/mediplus",
    "splash": {
      "image": "./assets/BlueLogoFull.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "bundleIdentifier": "com.heyalice.mediplus"
    }
  }
}
